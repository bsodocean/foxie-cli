generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Goodie {
  id String @id @default(nanoid())
  name String
  price Float
  createdAt DateTime @default(now())
  editedAt DateTime @updatedAt
  originalPrice Float?
  updatedPrice Float?
}